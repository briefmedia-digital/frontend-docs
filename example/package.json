{
  "name": "react-structure",
  "version": "1.0.0",
  "description": "Frontend styleguide",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --display-error-details",
    "dev": "NODE_ENV=development webpack --watch",
    "test-unit": "NODE_ENV=test mocha -w src/lib/tools/browser.js src/**/**.unit.spec.js",
    "test-integration": "NODE_ENV=test mocha -w src/lib/tools/browser.js src/**/**.integration.spec.js",
    "test-e2e": "NODE_ENV=test mocha -w src/lib/tools/browser.js src/**/**.e2e.spec.js",
    "test-all": "NODE_ENV=test mocha -w src/lib/tools/browser.js src/**/**.spec.js",
    "test": "NODE_ENV=test karma start karma.conf.js --singleRun",
    "test:watch": "NODE_ENV=test karma start karma.conf.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briefmedia-digital/frontend-docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/briefmedia-digital/frontend-docs/issues"
  },
  "homepage": "https://github.com/briefmedia-digital/frontend-docs#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^2.2.1",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.10.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^3.2.0",
    "normalize.css": "^5.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "postcss-responsify": "^1.0.3",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-addons-test-utils": "^15.4.2",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "requirejs": "^2.3.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "tachyons": "^4.6.2",
    "webpack": "^1.14.0",
    "webpack-merge": "^2.6.0",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "karma-webpack": "^2.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "globals": {
      "$": true
    },
    "rules": {
      "new-cap": 0,
      "padded-blocks": 0,
      "no-else-return": 0,
      "no-underscore-dangle": 0,
      "arrow-body-style": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-closing-bracket-location": 0
    }
  }
}
